{
    "name": "cortex/contacts",
    "description": "Cortex Contacts is a frontend layer for the polymorphic contact management system. You can add contacts to any eloquent model with ease.",
    "type": "cortex-module",
    "keywords": [
        "model",
        "rinvex",
        "cortex",
        "module",
        "contact",
        "laravel",
        "country",
        "primary",
        "billing",
        "shipping",
        "eloquent",
        "contacts",
        "polymorphic"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-contacts/issues",
        "source": "https://github.com/rinvex/cortex-contacts",
        "docs": "https://github.com/rinvex/cortex-contacts/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-contacts/contributors"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "Illuminate/auth": "^6.0.0",
        "Illuminate/console": "^6.0.0",
        "Illuminate/http": "^6.0.0",
        "Illuminate/routing": "^6.0.0",
        "Illuminate/support": "^6.0.0",
        "cortex/foundation": "^3.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3.0",
        "laravelcollective/html": "^6.0.0",
        "league/fractal": "^0.18.0",
        "proengsoft/laravel-jsvalidation": "^2.5.0",
        "rinvex/laravel-composer": "^3.0.0",
        "rinvex/laravel-contacts": "^3.0.0",
        "rinvex/laravel-menus": "^3.0.0",
        "rinvex/laravel-support": "^3.0.0",
        "rinvex/laravel-tenants": "^3.0.0",
        "silber/bouncer": "v1.0.0-rc.6",
        "spatie/laravel-activitylog": "^3.8.0",
        "spatie/laravel-schemaless-attributes": "^1.4.0",
        "yajra/laravel-datatables-buttons": "^4.8.0",
        "yajra/laravel-datatables-fractal": "^1.5.0",
        "yajra/laravel-datatables-html": "^4.16.0",
        "yajra/laravel-datatables-oracle": "^9.6.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Contacts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Contacts\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Contacts\\Providers\\ContactsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
