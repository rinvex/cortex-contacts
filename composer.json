{
    "name": "cortex/contacts",
    "type": "cortex-module",
    "description": "Cortex Contacts is a frontend layer for the polymorphic contact management system. You can add contacts to any eloquent model with ease.",
    "keywords": [
        "model",
        "rinvex",
        "cortex",
        "module",
        "contact",
        "laravel",
        "country",
        "primary",
        "billing",
        "shipping",
        "eloquent",
        "contacts",
        "polymorphic"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-contacts/issues",
        "source": "https://github.com/rinvex/cortex-contacts",
        "docs": "https://github.com/rinvex/cortex-contacts/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-contacts/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "laravel/framework": "~5.5.0",
        "rinvex/language": "dev-develop",
        "rinvex/contacts": "dev-develop",
        "rinvex/composable": "dev-develop",
        "cortex/foundation": "dev-develop"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Contacts\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Contacts\\Providers\\ContactsServiceProvider"
            ]
        },
        "cortex-module": {
            "autoinstall": false,
            "icon": {
                "color": "#2196f3",
                "name": "heartbeat",
                "background-size": "cover",
                "background-color": "#e3f2fd",
                "background-position": "center",
                "background-repeat": "no-repeat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
